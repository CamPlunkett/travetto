{
  "author": {
    "email": "<%= author.email %>",
    "name": "<%= author.name %>"
  },
  "dependencies": {
    <% for (const dep of depList) { %>"@travetto/<%= dep %>": "~<%= depVersion %>",<% } %>
    "@travetto/test": "~<%= depVersion %>"
  },
  "description": "A <%= modules.join(', ') %> based travetto project",
  "keywords": [<% for (const kw of keywords) {%>"<%= kw %>"<% }%>],
  "license": "MIT",
  "scripts": {
    "start": "npx travetto run <%= name %>",
    "test": "npx travetto test"
  },
  "files": [
    "src",
    "resources"
  ],
  "name": "<%= name %>",
  "version": "1.0.0"
}